<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <!-- Math Symbols -->
    <entry key="+">ADD</entry>
    <entry key="-">SUB</entry>
    <entry key="*">MUL</entry>
    <entry key="/">DIV</entry>
    <entry key="^">POW</entry>
    <entry key="%">PER_CEN</entry>
    <entry key="(">OPEN_P</entry>
    <entry key=")">CLOSE_P</entry>

    <!-- Seprater Symbols-->
    <entry key=",">SEP_C</entry>
    <entry key=";">SEP_SC</entry>
    <entry key="_">SUBS</entry>

    <!-- Basic Functions -->
    <entry key="sqrt">SQRT</entry>
    <entry key="sin">SIN</entry>
    <entry key="cos">COS</entry>
    <entry key="tan">TAN</entry>
    <entry key="arcsin">ASIN</entry>
    <entry key="arccos">ACOS</entry>
    <entry key="arctan">ATAN</entry>
    <entry key="log\(">LOG</entry>
    <entry key="ln">LN</entry>

    <entry key="sinh">SINH</entry>
    <entry key="cosh">COSH</entry>
    <entry key="tanh">TANH</entry>

    <entry key="csc">CSC</entry>
    <entry key="sec">SEC</entry>
    <entry key="cot">COT</entry>

    <entry key="deg">DEG</entry>
    <entry key="grad">GRAD</entry>
    <entry key="todeg">TO_DEG</entry>
    <entry key="tograd">TO_GRAD</entry>

    <!-- Functions -->
    <entry key="sum\(">SUM</entry>
    <entry key="pro\(">PRO</entry>

    <!-- Linear Algebra -->
    <entry key="vec\(">VEC</entry>
    <entry key="mat\(">MAT</entry>

    <!-- Constants -->
    <entry key="\\i">i</entry>
    <entry key="\\pi">pi</entry>
    <entry key="\\e">e</entry>
    <entry key="\\g">g</entry>
    <entry key="\\G">G</entry>

</properties>